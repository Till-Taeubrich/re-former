<h1>Sign Up</h1>

<%= form_with model: @user, url: users_path, method: :post do |form| %>
  <%= form.label :username %>
  <%= form.text_field :username %>
  <%= form.label :email %>
  <%= form.text_field :email %>
  <%= form.label :password %>
  <%= form.password_field :password %>
  <%= form.submit "Sign Up" %>
<% end %>
